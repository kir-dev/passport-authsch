{
  "name": "@kir-dev/passport-authsch",
  "version": "0.1.0",
  "description": "Passport.js Strategy for AuthSCH",
  "author": "SÃ¡muel Fekete <feketesamu@gmail.com>",
  "repository": "kir-dev/passport-authsch",
  "exports": {
    "require": "./build/cjs/index.js",
    "import": "./build/esm/index.js"
  },
  "types": "./build/cjs/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "node --import=@nitrogql/esbuild-register ./scripts/setup.ts",
    "build": "npm run clean && npm run build:cjs",
    "build:src": "node --import=@nitrogql/esbuild-register ./scripts/build.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json ",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf build coverage",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.esm.json",
    "type:check": "tsc --noEmit",
    "test": "node --import=@nitrogql/esbuild-register --test \"src/**/*.test.ts\"",
    "test:watch": "node --import=@nitrogql/esbuild-register --watch --test \"src/**/*.test.ts\"",
    "test:coverage": "NODE_V8_COVERAGE=./coverage c8 -r html node --import=@nitrogql/esbuild-register --experimental-test-coverage --test \"src/**/*.test.ts\"",
    "test:setup": "node --import=@nitrogql/esbuild-register ./scripts/test-setup.ts",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.0",
    "@nitrogql/esbuild-register": "^1.7.0",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^20.14.5",
    "@types/passport-strategy": "^0.2.38",
    "@types/prompts": "^2.4.9",
    "c8": "^10.1.2",
    "esbuild": "^0.21.5",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier": "^3.3.2",
    "prompts": "^2.4.2",
    "renamer": "^5.0.1",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "slugify": "^1.6.6",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "passport-strategy": "^1.0.0"
  }
}
